<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.camera"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:name="@string/app_name"
        android:icon="@drawable/logo" >
        <activity
            android:name="com.example.camera.StartActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.camera.HomeActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.example.camera.CameraActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen" >
        </activity>
        <activity android:name="com.example.camera.LoginActivity" >
        </activity>
        <activity
            android:name="com.example.camera.PhotoActivity">
        </activity>
        <activity
            android:name="com.example.camera.InformationActivity">
        </activity>
           <activity
            android:name="com.example.camera.RegisterActivity">
        </activity>
        
    </application>

</manifest>